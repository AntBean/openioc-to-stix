    <stix:Indicators>
        <stix:Indicator xmlns:indicator='http://stix.mitre.org/Indicator-2' xsi:type='indicator:IndicatorType' negate="false">
            <indicator:Title>CybOX-represented Indicator Created from OpenIOC File</indicator:Title>
            <indicator:Observable id="openioc:indicator-2e693207-ae90-4f9b-8a31-67f31f1d263c">
                <cybox:Title>This is a sample IOC that will hit on a number different artifacts present on a Windows computer. This IOC is used to test or illustrate the use of an IOC.</cybox:Title>
                <cybox:Observable_Composition operator="OR">
                    <cybox:Observable id="openioc:indicator-item-5ebfad1c-6f1a-472b-ae58-6fdfede0f4e7">
                        <cybox:Object>
                            <cybox:Properties xsi:type="FileObj:FileObjectType">
                                <FileObj:Full_Path condition="Contains">\kernel32.dll</FileObj:Full_Path>
                            </cybox:Properties>
                        </cybox:Object>
                    </cybox:Observable>
                    <cybox:Observable id="openioc:indicator-item-5b79c908-9d4d-4536-8699-9538af1576e8">
                        <cybox:Object>
                            <cybox:Properties xsi:type="FileObj:FileObjectType">
                                <FileObj:File_Name condition="Equals">win.ini</FileObj:File_Name>
                            </cybox:Properties>
                        </cybox:Object>
                    </cybox:Observable>
                    <cybox:Observable id="openioc:indicator-item-d1e188e1-fae6-488d-ba2f-a900abb21f14">
                        <cybox:Object>
                            <cybox:Properties xsi:type="FileObj:FileObjectType">
                                <FileObj:File_Extension condition="Contains">evt</FileObj:File_Extension>
                            </cybox:Properties>
                        </cybox:Object>
                    </cybox:Observable>
                    <cybox:Observable id="openioc:indicator-item-78af913e-a007-4f8a-864f-b543dd7a6d09">
                        <cybox:Object>
                            <cybox:Properties xsi:type="ProcessObj:ProcessObjectType">
                                <ProcessObj:Name condition="Equals">explorer.exe</ProcessObj:Name>
                            </cybox:Properties>
                        </cybox:Object>
                    </cybox:Observable>
                    <cybox:Observable id="openioc:indicator-item-9aaa6f73-5bd5-4dbe-b36f-73ee2e74655d">
                        <cybox:Object>
                            <cybox:Properties xsi:type="WinEventLogObj:WindowsEventLogObjectType">
                                <WinEventLogObj:EID condition="Equals">6009</WinEventLogObj:EID>
                            </cybox:Properties>
                        </cybox:Object>
                    </cybox:Observable>
                    <cybox:Observable id="openioc:indicator-item-16db1a80-af40-45fc-9db6-484bf372213b">
                        <cybox:Object>
                            <cybox:Properties xsi:type="UserAccountObj:UserAccountObjectType">
                                <UserAccountObj:Username condition="Equals">Administrator</UserAccountObj:Username>
                            </cybox:Properties>
                        </cybox:Object>
                    </cybox:Observable>
                    <cybox:Observable id="openioc:indicator-item-9a87eb7e-2b26-40a1-a8f6-7afa7d546aeb">
                        <cybox:Object>
                            <cybox:Properties xsi:type="WinServiceObj:WindowsServiceObjectType">
                                <WinServiceObj:Service_Name condition="Equals">TrkWks</WinServiceObj:Service_Name>
                            </cybox:Properties>
                        </cybox:Object>
                    </cybox:Observable>
                    <cybox:Observable id="openioc:indicator-item-0f605238-d7e1-4c5b-970c-071e50b82c22">
                        <cybox:Object>
                            <cybox:Properties xsi:type="WinRegistryKeyObj:WindowsRegistryKeyObjectType">
                                <WinRegistryKeyObj:Key condition="Contains">\DosDevices\C:</WinRegistryKeyObj:Key>
                            </cybox:Properties>
                        </cybox:Object>
                    </cybox:Observable>
                    <cybox:Observable id="openioc:indicator-item-e0b70fd0-38ac-4da2-a0eb-a9a9bb90e78f">
                        <cybox:Object>
                            <cybox:Properties xsi:type="PortObj:PortObjectType">
                                <PortObj:Port_Value condition="Equals">445</PortObj:Port_Value>
                            </cybox:Properties>
                        </cybox:Object>
                    </cybox:Observable>
                    <cybox:Observable id="openioc:indicator-item-faed6244-b531-46e1-9be9-baf19fc977b7">
                        <cybox:Object>
                            <cybox:Properties xsi:type="WinVolumeObj:WindowsVolumeObjectType">
                                <WinVolumeObj:Drive_Letter condition="Equals">C</WinVolumeObj:Drive_Letter>
                            </cybox:Properties>
                        </cybox:Object>
                    </cybox:Observable>
                    <cybox:Observable id="openioc:indicator-item-2e463fa6-fe0e-403c-9fa9-894bd42f8a82">
                        <cybox:Object>
                            <cybox:Properties xsi:type="DiskObj:DiskObjectType">
                                <DiskObj:Disk_Name condition="Equals">\\.\PhysicalDrive0</DiskObj:Disk_Name>
                            </cybox:Properties>
                        </cybox:Object>
                    </cybox:Observable>
                    <cybox:Observable id="openioc:indicator-item-a7fdaf04-51dc-4c7e-a1e1-d4baf40e863d">
                        <cybox:Object>
                            <cybox:Properties xsi:type="WinKernelHookObj:WindowsKernelHookObjectType">
                                <WinKernelHookObj:Hooked_Module condition="Equals">disk.sys</WinKernelHookObj:Hooked_Module>
                            </cybox:Properties>
                        </cybox:Object>
                    </cybox:Observable>
                    <cybox:Observable id="openioc:indicator-item-f23f9121-9618-43c0-860b-d0f7122e23de">
                        <cybox:Object>
                            <cybox:Properties xsi:type="WinDriverObj:WindowsDriverObjectType">
                                <WinDriverObj:Driver_Name condition="Equals">disk.sys</WinDriverObj:Driver_Name>
                            </cybox:Properties>
                        </cybox:Object>
                    </cybox:Observable>
                    <cybox:Observable id="openioc:indicator-990fbe29-6af6-45cb-b07e-6d13c5a30617">
                        <cybox:Observable_Composition operator="AND">
                            <cybox:Observable id="openioc:indicator-item-de7c6347-34d8-4a16-b559-38d9f4e6aabb">
                                <cybox:Object>
                                    <cybox:Properties xsi:type="FileObj:FileObjectType">
                                        <FileObj:File_Name condition="Equals">sens.dll</FileObj:File_Name>
                                    </cybox:Properties>
                                </cybox:Object>
                            </cybox:Observable>
                            <cybox:Observable id="openioc:indicator-item-96b8856c-f865-4805-93ed-aa8780b87617">
                                <cybox:Object>
                                    <cybox:Properties xsi:type="WinExecutableFileObj:WindowsExecutableFileObjectType">
                                        <WinExecutableFileObj:Digital_Signature signature_exists="true"/>
                                    </cybox:Properties>
                                </cybox:Object>
                            </cybox:Observable>
                        </cybox:Observable_Composition>
                    </cybox:Observable>
                </cybox:Observable_Composition>
